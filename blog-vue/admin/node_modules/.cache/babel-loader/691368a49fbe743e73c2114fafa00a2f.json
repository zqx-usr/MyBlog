EAAA,KAAA,EAAA,IAAA,CAAA,gBAAA;AAAA,oBAAA,IAAA,QAAA,IAAA;;AACA,oBAAA,IAAA,CAAA,IAAA,EAAA;AACA,kBAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA,EADA,CAEA;;AACA,kBAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,OAAA,EAAA,IAAA,CAAA,IAAA,EAHA,CAIA;AACA;;AACA,kBAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,kBAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,oBAAA,IAAA,EAAA;AAAA,mBAAA;AACA,iBARA,MAQA;AACA,kBAAA,IAAA,CAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA,eAZA;AAaA;AACA,WAtBA,CAAA,CAHA,CA2BA;;AACA,UAAA,OAAA,CAAA,IAAA;AACA,SA7BA,MA6BA;AACA,iBAAA,KAAA;AACA;AACA,OAjCA;AAkCA;AApCA;AAfA,CAAA","sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"login-card\">\r\n      <div class=\"login-title\">管理员登录</div>\r\n      <!-- 登录表单 -->\r\n      <el-form\r\n        status-icon\r\n        :model=\"loginForm\"\r\n        :rules=\"rules\"\r\n        ref=\"ruleForm\"\r\n        class=\"login-form\"\r\n      >\r\n        <!-- 用户名输入框 -->\r\n        <el-form-item prop=\"username\">\r\n          <el-input\r\n            v-model=\"loginForm.username\"\r\n            prefix-icon=\"el-icon-user-solid\"\r\n            placeholder=\"用户名\"\r\n            @keyup.enter.native=\"login\"\r\n          />\r\n        </el-form-item>\r\n        <!-- 密码输入框 -->\r\n        <el-form-item prop=\"password\">\r\n          <el-input\r\n            v-model=\"loginForm.password\"\r\n            prefix-icon=\"iconfont el-icon-mymima\"\r\n            show-password\r\n            placeholder=\"密码\"\r\n            @keyup.enter.native=\"login\"\r\n          />\r\n        </el-form-item>\r\n      </el-form>\r\n      <!-- 登录按钮 -->\r\n      <el-button type=\"primary\" @click=\"login\">登录</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import { generaMenu } from \"../../assets/js/menu\";\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      loginForm: {\r\n        username: \"\",\r\n        password: \"\"\r\n      },\r\n      rules: {\r\n        username: [\r\n          { required: true, message: \"用户名不能为空\", trigger: \"blur\" }\r\n        ],\r\n        password: [{ required: true, message: \"密码不能为空\", trigger: \"blur\" }]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      this.$refs.ruleForm.validate(valid => {\r\n        if (valid) {\r\n          const that = this;\r\n          // eslint-disable-next-line no-undef\r\n          var captcha = new TencentCaptcha(\r\n            this.config.TENCENT_CAPTCHA,\r\n            function(res) {\r\n              if (res.ret === 0) {\r\n                //发送登录请求\r\n                let param = new URLSearchParams();\r\n                param.append(\"username\", that.loginForm.username);\r\n                param.append(\"password\", that.loginForm.password);\r\n                that.axios.post(\"/api/login\", param).then(({ data }) => {\r\n                  if (data.flag) {\r\n                    that.$message.success(\"登录成功\");\r\n                    // 登录后保存用户信息\r\n                    that.$store.commit(\"login\", data.data);\r\n                    // 加载用户菜单\r\n                  //  generaMenu();\r\n                    that.$message.success(\"登录成功\");\r\n                    that.$router.push({ path: \"/\" });\r\n                  } else {\r\n                    that.$message.error(data.message);\r\n                  }\r\n                });\r\n              }\r\n            }\r\n          );\r\n          // 显示验证码\r\n          captcha.show();\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  right: 0;\r\n  left: 0;\r\n  /* todo 首页背景图 */\r\n  background: url(http://www.static.yehonghan.top/config/cover.jpg) center\r\n    center / cover no-repeat;\r\n}\r\n.login-card {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  right: 0;\r\n  background: #fff;\r\n  padding: 170px 60px 180px;\r\n  width: 350px;\r\n  opacity: 0.5;\r\n}\r\n.login-title {\r\n  color: #000;\r\n  font-weight: bold;\r\n  font-size: 2rem;\r\n}\r\n.login-form {\r\n  margin-top: 2rem;\r\n}\r\n.login-card button {\r\n  margin-top: 2rem;\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/login"}]}