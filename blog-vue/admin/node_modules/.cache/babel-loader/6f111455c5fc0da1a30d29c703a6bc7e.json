AA;AACA,WAAA,UAAA;AACA,KAnBA;AAoBA,IAAA,aApBA,yBAoBA,OApBA,EAoBA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,UAAA;AACA,KAvBA;AAwBA,IAAA,iBAxBA,6BAwBA,EAxBA,EAwBA;AAAA;;AACA,UAAA,KAAA,GAAA,EAAA;;AACA,UAAA,EAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,GAAA;AAAA,UAAA,MAAA,EAAA,KAAA,iBAAA;AAAA,UAAA,QAAA,EAAA;AAAA,SAAA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,GAAA;AAAA,UAAA,MAAA,EAAA,CAAA,EAAA,CAAA;AAAA,UAAA,QAAA,EAAA;AAAA,SAAA;AACA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,0BAAA,EAAA,KAAA,EAAA,IAAA,CAAA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,UAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;AACA,OAbA;AAcA,WAAA,gBAAA,GAAA,KAAA;AACA,KA9CA;AA+CA,IAAA,YA/CA,0BA+CA;AAAA;;AACA,WAAA,KAAA,CACA,MADA,CACA,mBADA,EACA;AAAA,QAAA,IAAA,EAAA,KAAA;AAAA,OADA,EAEA,IAFA,CAEA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,UAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;AACA,OAfA;AAgBA,WAAA,gBAAA,GAAA,KAAA;AACA,KAjEA;AAkEA,IAAA,oBAlEA,gCAkEA,GAlEA,EAkEA;AACA,WAAA,iBAAA,GAAA,GAAA,GAAA,KAAA,WAAA,GAAA,EAAA;AACA,WAAA,eAAA,GAAA,KAAA;AACA,KArEA;AAsEA,IAAA,wBAtEA,oCAsEA,KAtEA,EAsEA;AACA,UAAA,YAAA,GAAA,KAAA,CAAA,MAAA;AACA,WAAA,QAAA,GAAA,YAAA,KAAA,KAAA,WAAA,CAAA,MAAA;AACA,WAAA,eAAA,GACA,YAAA,GAAA,CAAA,IAAA,YAAA,GAAA,KAAA,WAAA,CAAA,MADA;AAEA;AA3EA,GAlBA;AA+FA,EAAA,KAAA,EAAA;AACA,IAAA,SADA,uBACA;AAAA;;AACA,WAAA,WAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,CAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,OAFA;AAGA;AANA;AA/FA,CAAA","sourcesContent":["<template>\r\n  <el-card class=\"main-card\">\r\n    <!-- 标题 -->\r\n    <div class=\"title\">{{ this.$route.name }}</div>\r\n    <!-- 相册操作 -->\r\n    <div class=\"operation\">\r\n      <div class=\"all-check\">\r\n        <el-checkbox\r\n          :indeterminate=\"isIndeterminate\"\r\n          v-model=\"checkAll\"\r\n          @change=\"handleCheckAllChange\"\r\n        >\r\n          全选\r\n        </el-checkbox>\r\n        <div class=\"check-count\">已选择{{ selectPhotoIdList.length }}张</div>\r\n      </div>\r\n      <el-button\r\n        type=\"success\"\r\n        @click=\"updatePhotoDelete(null)\"\r\n        :disabled=\"selectPhotoIdList.length == 0\"\r\n        size=\"small\"\r\n        icon=\"el-icon-deleteItem\"\r\n      >\r\n        批量恢复\r\n      </el-button>\r\n      <el-button\r\n        type=\"danger\"\r\n        @click=\"batchDeletePhoto = true\"\r\n        :disabled=\"selectPhotoIdList.length == 0\"\r\n        size=\"small\"\r\n        icon=\"el-icon-deleteItem\"\r\n      >\r\n        批量删除\r\n      </el-button>\r\n    </div>\r\n    <!-- 照片列表 -->\r\n    <el-row class=\"photo-container\" :gutter=\"10\" v-loading=\"loading\">\r\n      <!-- 空状态 -->\r\n      <el-empty v-if=\"photoList.length == 0\" description=\"暂无照片\" />\r\n      <el-checkbox-group\r\n        v-model=\"selectPhotoIdList\"\r\n        @change=\"handleCheckedPhotoChange\"\r\n      >\r\n        <el-col :md=\"4\" v-for=\"item of photoList\" :key=\"item.id\">\r\n          <el-checkbox :label=\"item.id\">\r\n            <div class=\"photo-item\">\r\n              <el-image\r\n                fit=\"cover\"\r\n                class=\"photo-img\"\r\n                :src=\"item.photoSrc\"\r\n                :preview-photoSrc-list=\"photoList\"\r\n              />\r\n              <div class=\"photo-name\">{{ item.photoName }}</div>\r\n            </div>\r\n          </el-checkbox>\r\n        </el-col>\r\n      </el-checkbox-group>\r\n    </el-row>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      :hide-on-single-page=\"true\"\r\n      class=\"pagination-container\"\r\n      @size-change=\"sizeChange\"\r\n      @current-change=\"currentChange\"\r\n      :current-page=\"current\"\r\n      :page-size=\"size\"\r\n      :total=\"count\"\r\n      layout=\"prev, pager, next\"\r\n    />\r\n    <!-- 批量删除对话框 -->\r\n    <el-dialog :visible.sync=\"batchDeletePhoto\" width=\"30%\">\r\n      <div class=\"dialog-title-container\" slot=\"title\">\r\n        <i class=\"el-icon-warning\" style=\"color:#ff9900\" />提示\r\n      </div>\r\n      <div style=\"font-size:1rem\">是否删除选中照片？</div>\r\n      <div slot=\"footer\">\r\n        <el-button @click=\"batchDeletePhoto = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"deletePhotos\">\r\n          确 定\r\n        </el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  created() {\r\n    this.listPhotos();\r\n  },\r\n  data: function() {\r\n    return {\r\n      loading: true,\r\n      batchDeletePhoto: false,\r\n      isIndeterminate: false,\r\n      checkAll: false,\r\n      photoList: [],\r\n      photoIdList: [],\r\n      selectPhotoIdList: [],\r\n      current: 1,\r\n      size: 18,\r\n      count: 0\r\n    };\r\n  },\r\n  methods: {\r\n    listPhotos() {\r\n      this.axios\r\n        .get(\"/api/admin/photos\", {\r\n          params: {\r\n            current: this.current,\r\n            size: this.size,\r\n            isDelete: 1\r\n          }\r\n        })\r\n        .then(({ data }) => {\r\n          this.photoList = data.data.recordList;\r\n          this.count = data.data.count;\r\n          this.loading = false;\r\n        });\r\n    },\r\n    sizeChange(size) {\r\n      this.size = size;\r\n      this.listPhotos();\r\n    },\r\n    currentChange(current) {\r\n      this.current = current;\r\n      this.listPhotos();\r\n    },\r\n    updatePhotoDelete(id) {\r\n      var param = {};\r\n      if (id == null) {\r\n        param = { idList: this.selectPhotoIdList, isDelete: 0 };\r\n      } else {\r\n        param = { idList: [id], isDelete: 0 };\r\n      }\r\n      this.axios.put(\"/api/admin/photos/delete\", param).then(({ data }) => {\r\n        if (data.flag) {\r\n          this.$notify.success({\r\n            title: \"成功\",\r\n            message: data.message\r\n          });\r\n          this.listPhotos();\r\n        } else {\r\n          this.$notify.error({\r\n            title: \"失败\",\r\n            message: data.message\r\n          });\r\n        }\r\n      });\r\n      this.batchDeletePhoto = false;\r\n    },\r\n    deletePhotos() {\r\n      this.axios\r\n        .delete(\"/api/admin/photos\", { data: this.selectPhotoIdList })\r\n        .then(({ data }) => {\r\n          if (data.flag) {\r\n            this.$notify.success({\r\n              title: \"成功\",\r\n              message: data.message\r\n            });\r\n            this.listPhotos();\r\n          } else {\r\n            this.$notify.error({\r\n              title: \"失败\",\r\n              message: data.message\r\n            });\r\n          }\r\n        });\r\n      this.batchDeletePhoto = false;\r\n    },\r\n    handleCheckAllChange(val) {\r\n      this.selectPhotoIdList = val ? this.photoIdList : [];\r\n      this.isIndeterminate = false;\r\n    },\r\n    handleCheckedPhotoChange(value) {\r\n      let checkedCount = value.length;\r\n      this.checkAll = checkedCount === this.photoIdList.length;\r\n      this.isIndeterminate =\r\n        checkedCount > 0 && checkedCount < this.photoIdList.length;\r\n    }\r\n  },\r\n  watch: {\r\n    photoList() {\r\n      this.photoIdList = [];\r\n      this.photoList.forEach(item => {\r\n        this.photoIdList.push(item.id);\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.operation {\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  margin-top: 2.25rem;\r\n  margin-bottom: 2rem;\r\n}\r\n.all-check {\r\n  display: inline-flex;\r\n  align-items: center;\r\n  margin-right: 1rem;\r\n}\r\n.check-count {\r\n  margin-left: 1rem;\r\n  font-size: 12px;\r\n}\r\n.photo-item {\r\n  position: relative;\r\n  cursor: pointer;\r\n  margin-bottom: 1rem;\r\n}\r\n.photo-img {\r\n  width: 100%;\r\n  height: 7rem;\r\n  border-radius: 4px;\r\n}\r\n.photo-name {\r\n  font-size: 14px;\r\n  margin-top: 0.3rem;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/album"}]}