eA,OAfA,EAeA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,cAAA;AACA,KAlBA;AAmBA,IAAA,cAnBA,0BAmBA,EAnBA,EAmBA;AAAA;;AACA,UAAA,KAAA,GAAA,EAAA;;AACA,UAAA,EAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,GAAA;AAAA,UAAA,IAAA,EAAA,KAAA;AAAA,SAAA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,GAAA;AAAA,UAAA,IAAA,EAAA,CAAA,EAAA;AAAA,SAAA;AACA;;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,uBAAA,EAAA,KAAA,EAAA,IAAA,CAAA,gBAAA;AAAA,YAAA,IAAA,QAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,cAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAdA;AAeA,KAzCA;AA0CA,IAAA,cA1CA,4BA0CA;AAAA;;AACA,WAAA,KAAA,CACA,GADA,CACA,uBADA,EACA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,OAAA,EAAA,KAAA,OADA;AAEA,UAAA,IAAA,EAAA,KAAA,IAFA;AAGA,UAAA,QAAA,EAAA,KAAA;AAHA;AADA,OADA,EAQA,IARA,CAQA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAZA;AAaA,KAxDA;AAyDA,IAAA,SAzDA,qBAyDA,QAzDA,EAyDA;AACA,UAAA,QAAA,IAAA,IAAA,EAAA;AACA,aAAA,YAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,QAAA,CAAA,CAAA;AACA,aAAA,KAAA,CAAA,aAAA,CAAA,SAAA,GAAA,MAAA;AACA,OAHA,MAGA;AACA,aAAA,YAAA,CAAA,EAAA,GAAA,IAAA;AACA,aAAA,YAAA,CAAA,YAAA,GAAA,EAAA;AACA,aAAA,KAAA,CAAA,aAAA,CAAA,SAAA,GAAA,MAAA;AACA;;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KAnEA;AAoEA,IAAA,iBApEA,+BAoEA;AAAA;;AACA,UAAA,KAAA,YAAA,CAAA,YAAA,CAAA,IAAA,MAAA,EAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA,eAAA,KAAA;AACA;;AACA,WAAA,KAAA,CACA,IADA,CACA,uBADA,EACA,KAAA,YADA,EAEA,IAFA,CAEA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,cAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,IAAA,CAAA;AAFA,WAAA;AAIA;;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,OAhBA;AAiBA;AA1FA;AArBA,CAAA","sourcesContent":["<template>\r\n  <el-card class=\"main-card\">\r\n    <div class=\"title\">{{ this.$route.name }}</div>\r\n    <!-- 表格操作 -->\r\n    <div class=\"operation-container\">\r\n      <el-button\r\n        type=\"primary\"\r\n        size=\"small\"\r\n        icon=\"el-icon-plus\"\r\n        @click=\"openModel(null)\"\r\n      >\r\n        新增\r\n      </el-button>\r\n      <el-button\r\n        type=\"danger\"\r\n        size=\"small\"\r\n        icon=\"el-icon-delete\"\r\n        :disabled=\"this.categoryIdList.length == 0\"\r\n        @click=\"isDelete = true\"\r\n      >\r\n        批量删除\r\n      </el-button>\r\n      <div style=\"margin-left:auto\">\r\n        <el-input\r\n          v-model=\"keywords\"\r\n          prefix-icon=\"el-icon-search\"\r\n          size=\"small\"\r\n          placeholder=\"请输入分类名\"\r\n          style=\"width:200px\"\r\n          @keyup.enter.native=\"searchCategories\"\r\n        />\r\n        <el-button\r\n          type=\"primary\"\r\n          size=\"small\"\r\n          icon=\"el-icon-search\"\r\n          style=\"margin-left:1rem\"\r\n          @click=\"searchCategories\"\r\n        >\r\n          搜索\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n    <!-- 表格展示 -->\r\n    <el-table\r\n      border\r\n      :data=\"categoryList\"\r\n      @selection-change=\"selectionChange\"\r\n      v-loading=\"loading\"\r\n    >\r\n      <!-- 表格列 -->\r\n      <el-table-column type=\"selection\" width=\"55\" />\r\n      <!-- 分类名 -->\r\n      <el-table-column prop=\"categoryName\" label=\"分类名\" align=\"center\" />\r\n      <!-- 文章量 -->\r\n      <el-table-column prop=\"articleCount\" label=\"文章量\" align=\"center\" />\r\n      <!-- 分类创建时间 -->\r\n      <el-table-column prop=\"createTime\" label=\"创建时间\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <i class=\"el-icon-time\" style=\"margin-right:5px\" />\r\n          {{ scope.row.createTime | date }}\r\n        </template>\r\n      </el-table-column>\r\n      <!-- 列操作 -->\r\n      <el-table-column label=\"操作\" width=\"160\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" size=\"mini\" @click=\"openModel(scope.row)\">\r\n            编辑\r\n          </el-button>\r\n          <el-popconfirm\r\n            title=\"确定删除吗？\"\r\n            style=\"margin-left:1rem\"\r\n            @confirm=\"deleteCategory(scope.row.id)\"\r\n          >\r\n            <el-button size=\"mini\" type=\"danger\" slot=\"reference\">\r\n              删除\r\n            </el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <el-pagination\r\n      class=\"pagination-container\"\r\n      background\r\n      @size-change=\"sizeChange\"\r\n      @current-change=\"currentChange\"\r\n      :current-page=\"current\"\r\n      :page-size=\"size\"\r\n      :total=\"count\"\r\n      :page-sizes=\"[10, 20]\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n    />\r\n    <!-- 批量删除对话框 -->\r\n    <el-dialog :visible.sync=\"isDelete\" width=\"30%\">\r\n      <div class=\"dialog-title-container\" slot=\"title\">\r\n        <i class=\"el-icon-warning\" style=\"color:#ff9900\" />提示\r\n      </div>\r\n      <div style=\"font-size:1rem\">是否删除选中项？</div>\r\n      <div slot=\"footer\">\r\n        <el-button @click=\"isDelete = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"deleteCategory(null)\">\r\n          确 定\r\n        </el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <!-- 添加编辑对话框 -->\r\n    <el-dialog :visible.sync=\"addOrEdit\" width=\"30%\">\r\n      <div class=\"dialog-title-container\" slot=\"title\" ref=\"categoryTitle\" />\r\n      <el-form label-width=\"80px\" size=\"medium\" :model=\"categoryForm\">\r\n        <el-form-item label=\"分类名\">\r\n          <el-input v-model=\"categoryForm.categoryName\" style=\"width:220px\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\">\r\n        <el-button @click=\"addOrEdit = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addOrEditCategory\">\r\n          确 定\r\n        </el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  created() {\r\n    this.listCategories();\r\n  },\r\n  data: function() {\r\n    return {\r\n      isDelete: false,\r\n      loading: true,\r\n      addOrEdit: false,\r\n      keywords: null,\r\n      categoryIdList: [],\r\n      categoryList: [],\r\n      categoryForm: {\r\n        id: null,\r\n        categoryName: \"\"\r\n      },\r\n      current: 1,\r\n      size: 10,\r\n      count: 0\r\n    };\r\n  },\r\n  methods: {\r\n    selectionChange(categoryList) {\r\n      this.categoryIdList = [];\r\n      categoryList.forEach(item => {\r\n        this.categoryIdList.push(item.id);\r\n      });\r\n    },\r\n    searchCategories() {\r\n      this.current = 1;\r\n      this.listCategories();\r\n    },\r\n    sizeChange(size) {\r\n      this.size = size;\r\n      this.listCategories();\r\n    },\r\n    currentChange(current) {\r\n      this.current = current;\r\n      this.listCategories();\r\n    },\r\n    deleteCategory(id) {\r\n      var param = {};\r\n      if (id == null) {\r\n        param = { data: this.categoryIdList };\r\n      } else {\r\n        param = { data: [id] };\r\n      }\r\n      this.axios.delete(\"/api/admin/categories\", param).then(({ data }) => {\r\n        if (data.flag) {\r\n          this.$notify.success({\r\n            title: \"成功\",\r\n            message: data.message\r\n          });\r\n          this.listCategories();\r\n        } else {\r\n          this.$notify.error({\r\n            title: \"失败\",\r\n            message: data.message\r\n          });\r\n        }\r\n        this.isDelete = false;\r\n      });\r\n    },\r\n    listCategories() {\r\n      this.axios\r\n        .get(\"/api/admin/categories\", {\r\n          params: {\r\n            current: this.current,\r\n            size: this.size,\r\n            keywords: this.keywords\r\n          }\r\n        })\r\n        .then(({ data }) => {\r\n          this.categoryList = data.data.recordList;\r\n          this.count = data.data.count;\r\n          this.loading = false;\r\n        });\r\n    },\r\n    openModel(category) {\r\n      if (category != null) {\r\n        this.categoryForm = JSON.parse(JSON.stringify(category));\r\n        this.$refs.categoryTitle.innerHTML = \"修改分类\";\r\n      } else {\r\n        this.categoryForm.id = null;\r\n        this.categoryForm.categoryName = \"\";\r\n        this.$refs.categoryTitle.innerHTML = \"添加分类\";\r\n      }\r\n      this.addOrEdit = true;\r\n    },\r\n    addOrEditCategory() {\r\n      if (this.categoryForm.categoryName.trim() == \"\") {\r\n        this.$message.error(\"分类名不能为空\");\r\n        return false;\r\n      }\r\n      this.axios\r\n        .post(\"/api/admin/categories\", this.categoryForm)\r\n        .then(({ data }) => {\r\n          if (data.flag) {\r\n            this.$notify.success({\r\n              title: \"成功\",\r\n              message: data.message\r\n            });\r\n            this.listCategories();\r\n          } else {\r\n            this.$notify.error({\r\n              title: \"失败\",\r\n              message: data.message\r\n            });\r\n          }\r\n          this.addOrEdit = false;\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/category"}]}