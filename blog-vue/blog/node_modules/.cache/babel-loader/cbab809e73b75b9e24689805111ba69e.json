A,GAAA,KAAA,CAAA,IAAA;AACA;;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,qBAAA,CAAA,MAAA,EAAA;AAAA;;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,qBAAA,CAAA,MAAA,CAAA;AACA,UAAA,KAAA,CAAA,OAAA;;AACA,+BAAA,KAAA,CAAA,WAAA,EAAA,IAAA,6CAAA,IAAA,CAAA,IAAA,CAAA,qBAAA;;AACA,UAAA,MAAA,CAAA,MAAA;AACA,SALA,MAKA;AACA,UAAA,MAAA,CAAA,QAAA;AACA;AACA,OArBA;AAsBA;AAxBA,GAlBA;AA4CA,EAAA,QAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,UAAA,KAAA,GAAA,EAAA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,SAAA,IAAA,aAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,SAAA;AACA;AACA,OAJA;AAKA,aAAA,qBAAA,KAAA,GAAA,mCAAA;AACA;AATA;AA5CA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 标签或分类名 -->\r\n    <div class=\"banner\" :style=\"cover\">\r\n      <h1 class=\"banner-title animated fadeInDown\">{{ title }} - {{ name }}</h1>\r\n    </div>\r\n    <div class=\"article-list-wrapper\">\r\n      <v-row>\r\n        <v-col md=\"4\" cols=\"12\" v-for=\"item of articleList\" :key=\"item.id\">\r\n          <!-- 文章 -->\r\n          <v-card class=\"animated zoomIn article-item-card\">\r\n            <div class=\"article-item-cover\">\r\n              <router-link :to=\"'/articles/' + item.id\">\r\n                <!-- 缩略图 -->\r\n                <v-img\r\n                  class=\"on-hover\"\r\n                  width=\"100%\"\r\n                  height=\"100%\"\r\n                  :src=\"item.articleCover\"\r\n                />\r\n              </router-link>\r\n            </div>\r\n            <div class=\"article-item-info\">\r\n              <!-- 文章标题 -->\r\n              <div>\r\n                <router-link :to=\"'/articles/' + item.id\">\r\n                  {{ item.articleTitle }}\r\n                </router-link>\r\n              </div>\r\n              <div style=\"margin-top:0.375rem\">\r\n                <!-- 发表时间 -->\r\n                <v-icon size=\"20\">mdi-clock-outline</v-icon>\r\n                {{ item.createTime | date }}\r\n                <!-- 文章分类 -->\r\n                <router-link\r\n                  :to=\"'/categories/' + item.categoryId\"\r\n                  class=\"float-right\"\r\n                >\r\n                  <v-icon>mdi-bookmark</v-icon>{{ item.categoryName }}\r\n                </router-link>\r\n              </div>\r\n            </div>\r\n            <!-- 分割线 -->\r\n            <v-divider></v-divider>\r\n            <!-- 文章标签 -->\r\n            <div class=\"tag-wrapper\">\r\n              <router-link\r\n                :to=\"'/tags/' + tag.id\"\r\n                class=\"tag-btn\"\r\n                v-for=\"tag of item.tagDTOList\"\r\n                :key=\"tag.id\"\r\n              >\r\n                {{ tag.tagName }}\r\n              </router-link>\r\n            </div>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n      <!-- 无限加载 -->\r\n      <infinite-loading @infinite=\"infiniteHandler\">\r\n        <div slot=\"no-results\" />\r\n        <div slot=\"no-more\" />\r\n      </infinite-loading>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  created() {\r\n    const path = this.$route.path;\r\n    if (path.indexOf(\"/categories\") != -1) {\r\n      this.title = \"分类\";\r\n    } else {\r\n      this.title = \"标签\";\r\n    }\r\n  },\r\n  data: function() {\r\n    return {\r\n      current: 1,\r\n      size: 10,\r\n      articleList: [],\r\n      name: \"\",\r\n      title: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    infiniteHandler($state) {\r\n      this.axios\r\n        .get(\"/api/articles/condition\", {\r\n          params: {\r\n            categoryId: this.$route.params.categoryId,\r\n            tagId: this.$route.params.tagId,\r\n            current: this.current\r\n          }\r\n        })\r\n        .then(({ data }) => {\r\n          if (data.data.name) {\r\n            this.name = data.data.name;\r\n            document.title = this.title + \" - \" + this.name;\r\n          }\r\n          if (data.data.articlePreviewDTOList.length) {\r\n            alert(data.data.articlePreviewDTOList.length);\r\n            this.current++;\r\n            this.articleList.push(...data.data.articlePreviewDTOList);\r\n            $state.loaded();\r\n          } else {\r\n            $state.complete();\r\n          }\r\n        });\r\n    }\r\n  },\r\n  computed: {\r\n    cover() {\r\n      var cover = \"\";\r\n      this.$store.state.blogInfo.pageList.forEach(item => {\r\n        if (item.pageLabel == \"articleList\") {\r\n          cover = item.pageCover;\r\n        }\r\n      });\r\n      return \"background: url(\" + cover + \") center center / cover no-repeat\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@media (min-width: 760px) {\r\n  .article-list-wrapper {\r\n    max-width: 1106px;\r\n    margin: 370px auto 1rem auto;\r\n  }\r\n  .article-item-card:hover {\r\n    transition: all 0.3s;\r\n    box-shadow: 0 4px 12px 12px rgba(7, 17, 27, 0.15);\r\n  }\r\n  .article-item-card:not(:hover) {\r\n    transition: all 0.3s;\r\n  }\r\n  .article-item-card:hover .on-hover {\r\n    transition: all 0.6s;\r\n    transform: scale(1.1);\r\n  }\r\n  .article-item-card:not(:hover) .on-hover {\r\n    transition: all 0.6s;\r\n  }\r\n  .article-item-info {\r\n    line-height: 1.7;\r\n    padding: 15px 15px 12px 18px;\r\n    font-size: 15px;\r\n  }\r\n}\r\n@media (max-width: 759px) {\r\n  .article-list-wrapper {\r\n    margin-top: 230px;\r\n    padding: 0 12px;\r\n  }\r\n  .article-item-info {\r\n    line-height: 1.7;\r\n    padding: 15px 15px 12px 18px;\r\n  }\r\n}\r\n.article-item-card {\r\n  border-radius: 8px !important;\r\n  box-shadow: 0 4px 8px 6px rgba(7, 17, 27, 0.06);\r\n}\r\n.article-item-card a {\r\n  transition: all 0.3s;\r\n}\r\n.article-item-cover {\r\n  height: 220px;\r\n  overflow: hidden;\r\n}\r\n.article-item-card a:hover {\r\n  color: #8e8cd8;\r\n}\r\n.tag-wrapper {\r\n  padding: 10px 15px 10px 18px;\r\n}\r\n.tag-wrapper a {\r\n  color: #fff !important;\r\n}\r\n.tag-btn {\r\n  display: inline-block;\r\n  font-size: 0.725rem;\r\n  line-height: 22px;\r\n  height: 22px;\r\n  border-radius: 10px;\r\n  padding: 0 12px !important;\r\n  background: linear-gradient(to right, #bf4643 0%, #6c9d8f 100%);\r\n  opacity: 0.6;\r\n  margin-right: 0.5rem;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/article"}]}